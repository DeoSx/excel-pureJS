{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "indent": "off",
    "no-unused-vars": "off",
    "no-trailing-spaces": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "eol-last": "off",
    "no-case-declarations": "off"
  }
}
